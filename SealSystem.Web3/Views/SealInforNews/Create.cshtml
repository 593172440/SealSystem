    <script type="text/javascript">
        function btnGenerate() {
            $("#main_row").append($("#demo_row").prop("outerHTML"))
        }
        function delDevelopmentNumber(e) {
            console.log($(e).parent())
            $(e).parent().parent().parent().parent().parent().remove();
        }
        function btnSubmit(e) {
            var form_object = $(e).parent().parent().parent();
            console.log(form_object.serialize());
        }
        function btnSubmitAll() {
            if ($("#SealCategory_Id_Code option:selected").val()== "请选择") {
                return alert("请选择印章类型")
            }
            if (app.Attention == "") {
                return alert("经办人不能为空");
            }
            if (app.Approval == "") {
                return alert("备案人不能为空");
            }
            if (app.AttentionIdCard == "") {
                return alert("身份证不能为空");
            }
            all_result = []
            var form_index = 0;
            $(".form_cls").each(function () {
                var temp_dict = {}
                $(this).find("input").each(function () {
                    $.each($(this).serializeArray(), function (i, field) {
                        temp_dict[field.name] = field.value;
                    });
                })
                $(this).find("select").each(function () {
                    $.each($(this).serializeArray(), function (i, field) {
                        temp_dict[field.name] = field.value;
                    });
                })
                temp_dict["form_index"] = String(form_index);
                all_result.push(temp_dict);
                form_index += 1;
            })

            var arss = {
                SealUseUnitInfor_Id_UnitNumber: $("#SealUseUnitInfor_Id_UnitNumber").val(),//使用单位//这里获取到是使用单位,需要后台转换为id属性
                SealState: app.SealState,//印章状态
                SealApprovalUnitInfor_Id_ApprovalUnitCode: $("#SealApprovalUnitInfor_Id_ApprovalUnitCode option:selected").val(),//备案单位//这里获取到的是下拉列表中的vlue值(1/2),不需要转换,直接插入数据及可
                SealMakingUnitInfor_Id_MakingUnitCode:$("#SealMakingUnitInfor_Id_MakingUnitCode option:selected").val(),//制作单位//这里获取到的是下拉列表中的vlue值(1/2),不需要转换,直接插入数据及可
                ApprovalTime: $("#ApprovalTime").val(),//备案日期
                Attention: $("#Attention").val(),//经办人
                AttentionIdCard: $("#AttentionIdCard").val(),//身份证
                Approval: $("#Approval").val(),//备案人
                ForeignLanguageContent: $("#ForeignLanguageContent").val()//外文内容
            }
            console.log(JSON.stringify(all_result));

            console.log(arss);

            $.ajax({
                type: "post",
                url: "/SealInforNews/CreateSealList",
                data: {
                    "seal": JSON.stringify(all_result),
                    "ars": JSON.stringify(arss)
                },
                traditional: true,
                dataType: "json",
