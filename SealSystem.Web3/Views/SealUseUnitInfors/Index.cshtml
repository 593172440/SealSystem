@model IEnumerable<SealSystem.Models.SealUseUnitInfor>
<div class="container body">
    <div class="main_container">
        <!--右上角头像区-->
        @Html.Partial("~/Views/Shared/_PartialTopHeadImages.cshtml");
        <!--/右上角头像区-->
        <!--顶部导航-->
        <div class="top_nav">
            <div class="nav_menu">
                @Html.Partial("~/Views/Shared/_PartialTopNavigation.cshtml");
            </div>
        </div>
        <!--/顶部导航 -->
        <!-- page content -->
        <div class="right_col" role="main" style=null>
            <div class="">

                <div class="page-title">
                    <div class="title_left">

                    </div>

                    <div class="title_right">
                        <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">

                        </div>
                    </div>
                </div>

                <div class="clearfix"></div>

                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_title">
                                <h2><span class="glyphicon glyphicon-plus"></span>@Html.ActionLink("增加新单位", "Create")</h2>
                                <ul class="nav navbar-right panel_toolbox">
                                    <li>
                                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                    </li>
                                    <li class="dropdown">
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button"
                                           aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>
                                                <a href="#">Settings 1</a>
                                            </li>
                                            <li>
                                                <a href="#">Settings 2</a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a class="close-link"><i class="fa fa-close"></i></a>
                                    </li>
                                </ul>
                                <div class="clearfix"></div>
                            </div>
                            <div class="x_content">
                                <table id="datatable" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>使用单位编号</th>
                                            <th>使用单位名称</th>
                                            <th>所属区域</th>
                                            <th>企业类型</th>
                                            <th>企业法人</th>
                                            <th>法人身份证</th>
                                            <th>备案日期</th>
                                            <th></th>
                                        </tr>
                                    </thead>

                                    <tbody>

                                        @foreach (var item in Model)
                                        {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.UnitNumber)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Area.Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.SealUnitCategory.Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.LegelPerson)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IdNumber)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.CreateTime)
                                            </td>

                                            <td>
                                                @Html.ActionLink("编辑", "Edit", new { id = item.Id }) |
                                                @*@Html.ActionLink("查看", "Details", new { id = item.Id }) |*@
                                                @Html.ActionLink("删除", "Delete", new { id = item.Id })
                                            </td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <!-- /page content -->
        @Html.Partial("~/Views/Shared/_PartialFooter.cshtml");
    </div>
</div>
